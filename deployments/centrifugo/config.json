{
  "engine": "redis",
  "redis_address": "redis:6379",
  "token_hmac_secret_key": "local-centrifugo-secret",
  "api_key": "local-centrifugo-key",
  "grpc_api": true,
  "grpc_api_address": "0.0.0.0:8001",
  "allowed_origins": ["http://localhost:5173"],
  "log_level": "debug",
  "health": true,
  "namespaces": [
    {
      "name": "user",
      "proxy_subscribe": {
        "enabled": true,
        "endpoint": "grpc://host.docker.internal:8080"
      }
    },
    {
      "name": "match",
      "proxy_subscribe": {
        "enabled": true,
        "endpoint": "grpc://host.docker.internal:8080"
      }
    }
  ],
  "rpc": {
    "proxy": {
      "enabled": true,
      "endpoint": "grpc://host.docker.internal:8080"
    }
  }
}